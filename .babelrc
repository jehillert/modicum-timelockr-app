// NOTE: "babel-preset-react-native has been replaced by metro-react-native-babel-preset"
{
    "presets": ["module:metro-react-native-babel-preset"],
    "env": {
        "production": {
          "plugins": ["react-native-paper/babel"]
        }
    },
    "plugins": [
        [
            "babel-plugin-styled-components"
        ],
        [
            "module-resolver",
            {
                "root": ["./src"],
                "extensions": [".js", ".ios.js", ".android.js"],
                "alias": {
                    "@assets": "./assets",
                    "@components": "./src/components/components-index",
                    "@constants": "./src/constants",
                    "@env": "@env",
                    "@hooks": "./src/hooks",
                    "@navigation": "./src/components/navigation/index",
                    "@noke-slices": "./src/noke/noke-slices",
                    "@noke": "./src/noke/index",
                    "@reducers": "./src/redux",
                    "@slices": "./src/components/slices-index",
                    "@root-reducer": "./src/redux/root-reducer",
                    "@screens": "./src/components/screens-index",
                    "@selectors": "./src/redux/selectors",
                    "@src": "./src",
                    "@store": "./src/redux/store",
                    "@styled": "./src/components/styled",
                    "@theme": "./src/theme/index",
                    "@utilities": "./src/util"
                }
            }
        ],
        [
            "module:react-native-dotenv",
            {
                "moduleName": "@env",
                "path": ".env",
                "blacklist": null,
                "whitelist": null,
                "safe": false,
                "allowUndefined": true
            }
        ]
    ]
}
